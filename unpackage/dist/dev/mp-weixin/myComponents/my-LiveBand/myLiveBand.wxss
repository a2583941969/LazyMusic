
.Live-Band {
  margin-bottom: 100rpx;
  margin-right: 20rpx;
}
.Live-Band>.Singer-info {
  width: 196rpx;
  height: 314rpx;
}
.Live-Band>.Singer-info>navigator>.Singer-avatar {
  width: 196rpx;
  height: 196rpx;
  border-radius: 50%;
  overflow: hidden;
}
.Live-Band>.Singer-info>navigator>.Singer-avatar>image {
  width: 125%;
  height: 125%;
  display: block;
  margin-left: -25rpx;
}
.Live-Band>.Singer-info>navigator>.Leigh-Nash {
  width: 196rpx;
  height: 44rpx;
  margin-top: 24rpx;
  text-align: center;
  font-size: 28rpx;
  font-weight: 600;
  color: #333333;
}
.Live-Band>.Singer-info>navigator>.collect-num {
  width: 196rpx;
  height: 44rpx;
  text-align: center;
  font-size: 22rpx;
  color: #AAAAAA;
  font-weight: 100;
}
.Live-Band>.Singer-info>.collect {
  text-align: center;
}
.Live-Band>.Singer-info>.collect>text {
  font-size: 20rpx;
  border-radius: 40rpx;
  border: 1px solid #af9b7c;
  padding: 6rpx 36rpx;
  color: #af9b7c;
}
.Live-Band>.Singer-info>.collect>text.collectActive {
  color: #FFFFFF;
  background-color: #af9b7c;
}

